# -*-shell-script-*-

# Setup minimal sbtools environment (sets PATH, LOCATION, HOST, USER, ...)
. /mathworks/hub/share/sbtools/bash_setup_env.bash

xrdb -merge ~/.Xresources &

export WINDOW_MANAGER=xmonad

xcompmgr &

gnome-keyring-daemon &
gnome-settings-daemon &
(sleep 5 && gnome-screensaver) &

bash $HOME/bin/wallpaper.sh &
#feh --bg-fill /home/spenser/Pictures/high-res/Air___Exo-3840x1200.jpg &
sleep 2 && setxkbmap -option "caps:swapescape"
xset b off

